<h1>This is the movies page</h1>
<a href="/celebrities/create">Add a movie to our list</a>
{{#each movies}}
    <div class="celebrity-card">
        <h3>{{this.title}}</h3>
        <p>Occupation: {{this.genre}}</p>
        <p>Catch phrase: {{this.plot}}</p>
        <ul>
            {{#each this.cast}}
                <li>{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
{{/each}}